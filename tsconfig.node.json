{
	"compilerOptions": {
		"target": "ESNext",
		"useDefineForClassFields": true,
		"module": "ESNext",
		"moduleResolution": "Node",
		"allowImportingTsExtensions": true,
		"isolatedModules": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"noEmit": true,
		"strict": true,
		"lib": ["ESNext", "DOM"],
		"skipLibCheck": true
	}
}
